{
  "name": "@crystallize/react-video",
  "description": "Embed vidoes from Crystallize in React",
  "author": "HÃ¥kon Krogh",
  "version": "1.7.0",
  "keywords": [
    "react",
    "video",
    "crystallize",
    "hls",
    "dash"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./dist/styles.css": "./dist/styles.css"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && cp src/styles.css dist/styles.css",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16",
    "@crystallize/react-image": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react-video.esm.js",
  "size-limit": [
    {
      "path": "dist/react-video.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-video.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@crystallize/react-image": ">=6.0.2",
    "babel-loader": "^8.2.2",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CrystallizeAPI/react-video.git"
  },
  "release": {
    "branches": [
      "main",
      "next",
      "beta"
    ]
  }
}
